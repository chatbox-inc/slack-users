    "require": {
        "chatbox-inc/laravel-webhook": "*@dev",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "laravel/laravel": "^5.8",
        "phpunit/phpunit": "^8.1",
        "fzaninotto/faker": "^1.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Chatbox\\Slackin\\SlackinServiceProvider"
            ]
        }
    }
}
