{
    "name": "chatbox-inc/laravel-slackin",
    "authors": [
        {
            "name": "mikakane",
            "email": "t.goto@chatbox-inc.com"
        }
    ],
    "repositories": [{
        "type": "path",
        "url": "../laravel-webhook"
    }
